    </field>
    <field name="country" type="string" column="country" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="searchText" type="string" column="search_text" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tenantId" type="string" column="tenant_id" length="31" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
